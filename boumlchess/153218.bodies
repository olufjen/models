class LogSigActivationFunction
!!!245506.java!!!	activation(in parameter : double) : double

		return 1.0 / (1.0 + Math.pow(Math.E, (-1.0 * parameter)));
!!!245634.java!!!	deriv(in parameter : double) : double
		// parameter = induced field
		// e == activation
		double e = 1.0 / (1.0 + Math.pow(Math.E, (-1.0 * parameter)));
		return e * (1.0 - e);

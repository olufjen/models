	  param inout name "algo" type class_ref 137730 // SearchForActions
	
	preserve_java_body_indent java_def "  ${comment}${@}${visibility}${final}${static}${abstract}${synchronized}${type} ${name}${(}${t0} ${p0}, ${t1}<NQueensBoard, QueenAction> ${p1}${)}${throws}${staticnl}{
